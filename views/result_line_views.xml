<odoo>
    <record id="view_vote_management_result_line_form" model="ir.ui.view">
        <field name="name">vote_management.result_line.form</field>
        <field name="model">vote_management.result_line</field>
        <field name="arch" type="xml">
            <form string="Party Result Line" readonly="true">
                <sheet>
                    <group>
                        <field name="party_id"/>
                        <field name="votes"/>
                        <field name="num_mps"/>
                        <field name="election_result_id"/>
                        <field name="district_result_id"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>